    "test": "xo",
    "fix": "xo --fix"
  },
  "dependencies": {
    "nextkit": "^3.1.0"
  },
  "peerDependencies": {
    "next": "*"
  },
  "devDependencies": {
    "next": "^13.0.5",
    "tsup": "^6.0.1",
    "typedoc": "^0.23.10",
    "typescript": "^5.1.3",
    "xo": "^0.53.1"
  }
}
